<body>
    <h1>Thêm mới sản phẩm</h1>
    <form action="/categories" method="post" class="createForm">
        <input name ="picture" type="text" placeholder="Ảnh"> 
        <input name ="name" type="text" placeholder="Tên danh mục"> 
        <input name ="description" type="text" placeholder="Mô tả"> 
        <input name ="price" type="text" placeholder="Giá"> 
        <input name ="ammount" type="text" placeholder="Số lượng"> 
        <input name="createTime" type="text" placeholder="Thời gian tạo">
        <input name="updateTime" type="text" placeholder="Thời gian cập nhật">
        <input name="status" type="text" placeholder="Trạng thái">
        <button>Tạo mới</button>
    </form>
